{
    "name": "yiisoft/yii-debug",
    "description": "The debugger extension for the Yii framework",
    "keywords": ["yii", "debug", "debugger"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-debug/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-debug"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii-bootstrap-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\debug\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "src/config/params.php",
            "common": "src/config/common.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
